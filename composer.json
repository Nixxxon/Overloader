{
    "name": "nixxxon/overloader",
    "type": "library",
    "description": "Overloader - Overload object methods and properties",
    "keywords": [
        "overload",
        "override"
    ],
    "homepage": "https://github.com/nixxxon/overloader",
    "license": "MIT",
    "authors": [
        {
            "name": "Niclas Ã…berg",
            "email": "niclas.aberg@gmail.com",
            "homepage": "https://github.com/nixxxon",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Nixxxon\\Overloader\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nixxxon\\Overloader\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
